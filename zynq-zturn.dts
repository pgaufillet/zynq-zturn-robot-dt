/dts-v1/;
/include/ "zynq-7000.dtsi"


/ {
	model = "MYIR Z-turn Development Board";
	compatible = "myir,zynq-zturn", "xlnx,zynq-7000";

	aliases {
		ethernet0 = &gem0;
		serial0 = &uart1;
		serial1 = &uart0;
		spi0 = &qspi;
	};

	memory@0 {
		device_type = "memory";
		reg = <0x0 0x40000000>;
	};

	chosen {
		bootargs = "";
		stdout-path = "serial0:115200n8";
	};
};

&clkc {
	ps-clk-frequency = <33333333>;
};

&gpio0 {
    emio-gpio-width = <64>;
    gpio-mask-high = <0x0>;
    gpio-mask-low = <0x5600>;
    xlnx,emio-gpio-width = <0x40>;
    xlnx,mio-gpio-mask = <0x5600>;
};

&qspi {
	status = "okay";
	is-dual = <0>;
	num-cs = <1>;
	xlnx,fb-clk = <0x1>;
	xlnx,qspi-mode = <0x0>;
	flash@0 {
		compatible = "n25q128";
		reg = <0x0>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <4>;
		spi-max-frequency = <50000000>;
		#address-cells = <1>;
		#size-cells = <1>;
		partition@qspi-boot {
			label = "qspi-boot";
			reg = <0x0 0x080000>;
		};
		partition@qspi-bootenv {
			label = "qspi-bootenv";
			reg = <0x080000 0x020000>;
		};		
		partition@qspi-bitstream {
			label = "qspi-bitstream";
			reg = <0x0A0000 0x460000>;
		};		
		partition@qspi-kernel {
			label = "qspi-kernel";
			reg = <0x500000 0x480000>;
		};
		partition@qspi-devicetree {
			label = "qspi-devicetree";
			reg = <0x980000 0x010000>;
		};
		partition@qspi-rootfs {
			label = "qspi-rootfs";
			reg = <0x990000 0x600000>;
		};
		partition@qspi-data {
			label = "data";
			reg = <0xF90000 0x070000>;
		};
	};
};

&usb0 {
	status = "okay";
	dr_mode = "host";
	phy_type = "ulpi";	
};

&gem0 {
    status = "okay";
    interrupt-parent = <&intc>;
    phy-mode = "rgmii-id";
    phy-handle = <&phy0>;
    phy0: phy@0 {
        compatible = "atheros,ar8035";
        device_type = "ethernet-phy";
        reg = <0>;
    };
};

&sdhci0 {
	u-boot,dm-pre-reloc;
	status = "okay";
    xlnx,has-cd = <0x1>;
    xlnx,has-power = <0x0>;
    xlnx,has-wp = <0x1>;
};

&uart0 {
	u-boot,dm-pre-reloc;
	status = "okay";
    current-speed = <115200>;
    port-number = <1>;
    xlnx,has-modem = <0x0>;
};

&uart1 {
	u-boot,dm-pre-reloc;
	status = "okay";
    current-speed = <115200>;
    port-number = <0>;
    xlnx,has-modem = <0x0>;
};

&i2c0 {
	status = "okay";
    clock-frequency = <400000>;
	
    adxl345@53 {
        compatible = "adi,adxl34x","adxl34x";
        reg = <0x53>;
        interrupt-parent = <&intc>;
        interrupts = < 0 30 4 >;
    };

    sii902x@3b {
        compatible = "sii902x";
        reg = <0x3b>;
        interrupt-parent = <&intc>;
        interrupts = < 0 29 4 >;
		mode_str = "1920x1080";
		bits-per-pixel = <16>;
    };

	stlm75@49 {
		status = "okay";
		compatible = "lm75";
		reg = <0x49>;
/*		interrupts = < 0 32 4 >;*/
	};
};

